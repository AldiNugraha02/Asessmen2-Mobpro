<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="org.d3if0144.kalkulatorlite.ui.main.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_resultFragment"
            app:destination="@id/resultFragment" />
        <action
            android:id="@+id/action_mainFragment_to_historiFragment"
            app:destination="@id/historiFragment" />
    </fragment>
    <fragment
        android:id="@+id/resultFragment"
        android:name="org.d3if0144.kalkulatorlite.ui.result.ResultFragment"
        android:label="ResultFragment"
        tools:layout="@layout/fragment_result"/>
    <fragment
        android:id="@+id/historiFragment"
        android:name="org.d3if0057.calculator.ui.histori.HistoriFragment"
        android:label="HistoriFragment" />
</navigation>